{
	"name": "Welcome Box",
	"description": "Shows Welcome Box with information like guilds, houses, players on the front page (news).",
	"version": "1.1",
	"type": "plugin",
	"license": "GPLv3",
	"author": "slawkens",
	"contact": "slawkens@gmail.com",
	"require": {
		"myaac": "1.0-beta",
		"php": "8.1"
	},
	"hooks": {
		"Welcome Box": {
			"type": "BEFORE_PAGE",
			"file": "plugins/welcome-box/welcome-box.php"
		}
	},
	"uninstall": [
		"plugins/welcome-box.json",
		"plugins/welcome-box"
	]
}
